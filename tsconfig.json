{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "moduleResolution": "node",
	"isolatedModules": true,
    "esModuleInterop": true,
	"noImplicitAny": true,
    "types": [
      "@cloudflare/workers-types",
    ]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
